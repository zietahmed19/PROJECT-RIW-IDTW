{
  "name": "copilotanswers",
  "displayName": "GitHub Copilot",
  "description": "helper",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateAnswers",
        "title": "Generate Answers from Stack Overflow Questions"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "extension.csvFilePath": {
          "type": "string",
          "default": "",
          "description": "Path to the CSV file containing Stack Overflow questions"
        },
        "extension.outputFilePath": {
          "type": "string",
          "default": "",
          "description": "Path to the file where answers will be saved"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
